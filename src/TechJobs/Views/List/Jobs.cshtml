<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@{
    @for (int i = 0; i < ViewBag.jobs.Count; i++)
    {

       
        
            <table class="job-listing">
                <tbody>
                    @foreach (KeyValuePair<string, string> j in ViewBag.jobs[i])
                    {
                        <tr>
                            <td>@j.Key</td>
                            <td>@j.Value</td>
                        </tr>

                    }

                </tbody>
            </table>
        

    }
        // TODO #3 - Use the same list display from Views/Search/Index.cshtml
}